<h1>New Contact</h1>

<%= form_for([@person, @contact]) do |f| %>
  <p>

  <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name%>
  </p>

  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <%= f.label :age%><br>
    <%= f.text_field :age %>
  </p>

  <p>
    <%= f.label :ethnicity %><br>
    <%= f.text_field :ethnicity %>
  </p>

  <p>
    <%= f.label :origin %><br>
    <%= f.text_field :origin %>
  </p>

  <p>
    <%= f.label :gender %><br>
    <%= f.text_field :gender %>
  </p>

  <p>
    <%= f.label :occupation %><br>
    <%= f.text_field :occupation %>
  </p>

  <p>
    <%= f.label :company %><br>
    <%= f.text_field :company %>
  </p>

  <p>
    <%= f.label :year_met %><br>
    <%= f.text_field :year_met %>
  </p>

  <p>
    <%= f.label :place_met %><br>
    <%= f.text_field :place_met %>
  </p>

  <p>
    <%= f.label :last_in_contact %><br>
    <%= f.text_field :last_in_contact %>
  </p>

  <p>
    <%= f.label :network_size %><br>
    <%= f.text_field :network_size %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
